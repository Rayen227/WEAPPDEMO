page {
  margin: 0;
  padding: 0;
  background-color: cornflowerblue;
}
/* 头像 */
.head {
  box-sizing: border-box;
  margin: 90rpx auto -40rpx;
  width: 160rpx;
  height: 160rpx;
  border-radius: 80rpx;
  background-color: coral;
}
.head image {
  box-sizing: border-box;
  width: 160rpx;
  height: 160rpx;
  border-radius: 80rpx;
}

/* 排名 */
.rank {
  margin: 70rpx auto;
  width: 80%;
  height: 300rpx;
  background-color: darkgoldenrod;
}

/* 开始 */
.game {
  margin: 80rpx auto;
  width: 400rpx;
  height: 130rpx;
  background-color: darkgoldenrod;
  text-align: center;
  line-height: 130rpx;
}

/* 空间 */
.room {
  margin: 80rpx auto;
  width: 400rpx;
  height: 130rpx;
  background-color: darkgoldenrod;
  text-align: center;
  line-height: 130rpx;
}

/* 错题本 */
.mistake {
  position: absolute;
  left: 20rpx;
  bottom: 20rpx;
  width: 150rpx;
  height: 150rpx;
  background-color: bisque;
}
