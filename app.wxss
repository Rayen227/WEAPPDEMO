/* 音量按键设置 */
.musicSetup{
  position: fixed;
  top: 0;
  right: 0;
  width: 90rpx;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  /* color: #681752;  暗玉紫 */
  background-color: lightcoral;
}
/* 选中后 */
    /* 权值问题 */
.select {
  color: rgb(94, 255, 0);
  background-color: lightcyan;
}

.position {
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
}
