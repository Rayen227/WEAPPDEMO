@import "weui.wxss";

/* 字体图标 */
@import "iconfont.wxss";
.iconfont {
  font-family: "iconfont" !important;
  font-size: 24px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

/* 字体样式 */
/* @import "FZSJ-MUJHXYJX.wxss"; */
.font {
  src: url(https://7465-test-h043w-1301939913.tcb.qcloud.la/font/FZSJ-MUJHXYJX.TTF?sign=202e13e6a20c5fc7029cbf3909d1f128&t=1589643098);
  font-family: 'FZSJ-MUJHXYJX';
  font-size: 62rpx;
